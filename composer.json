{
    "name": "tonis-io/pimple",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "container-interop/container-interop": "^1.1",
        "pimple/pimple": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Tonis\\Pimple\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tonis\\Pimple\\": [
                "test/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
